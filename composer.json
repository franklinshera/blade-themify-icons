{
  "name": "franklinshera/blade-themify-icons",
  "description": "A package to easily make use of Themify Icons in your Laravel Blade views.",
  "keywords": [
    "Blade",
    "Themify Icons",
    "Laravel"
  ],
  "homepage": "https://github.com/franklinshera/blade-themify-icons",
  "license": "MIT",
  "authors": [
    {
      "name": "Franklin Shera",
      "email": "fshera96@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "blade-ui-kit/blade-icons": "^1.1",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^6.0",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "BladeUI\\ThemifyIcons\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "BladeUI\\ThemifyIcons\\BladeThemifyIconsServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
